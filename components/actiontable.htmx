<table style="border: 1px solid;" id="${this.id}">
    <thead>
    $for (column in $comp.column) {
        <th>${column.title}</th>
    }
    </thead>
    <tbody>
    $for (item in $comp.list with index){
        <tr>
            $for (column in $comp.column) {
                <td>${eval(column.content)}</td>
            }
            <td><button onclick="$comp.remove(index)">Remove</button></td>
        </tr>
    }
    </tbody>
</table>
<br>